{
    "name": "jsec516/simple-acl",
    "description": "Simple-acl is a simple access control library, which is framework agnostic",
    "license": "MIT",
    "keywords": ["simple-acl"],
    "authors": [
        {
            "name": "Jahidul Islam"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
    	"psr-0": {
    		"": "src"
    	},
    	"psr-4": {
            "SimpleAcl\\": "src/SimpleAcl/"
        },
        "files": [
            "tests/TestFixture.php"
        ]
    }
}